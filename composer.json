{
    "name": "marksitko/laravel-unsplash",
    "description": "Provides a fluent Unsplash API for Laravel",
    "keywords": [
        "marksitko",
        "laravel-unsplash"
    ],
    "homepage": "https://github.com/marksitko/laravel-unsplash",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mark Sitko",
            "email": "mark@sitko-designing.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Marksitko\\LaravelUnsplash\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Marksitko\\LaravelUnsplash\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marksitko\\LaravelUnsplash\\LaravelUnsplashServiceProvider"
            ],
            "aliases": {
                "LaravelUnsplash": "Marksitko\\LaravelUnsplash\\LaravelUnsplashFacade"
            }
        }
    }
}
